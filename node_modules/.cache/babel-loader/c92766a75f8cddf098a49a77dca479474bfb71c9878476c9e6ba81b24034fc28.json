{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'AppDisplayQuestion',\n  data() {\n    return {\n      Posts: [],\n      nombrePost: 0,\n      page: 1,\n      count: 0,\n      nbPage: 1\n    };\n  },\n  mounted() {\n    this.fetchPosts();\n  },\n  methods: {\n    fetchPosts() {\n      axios.get(`http://127.0.0.1:8000/api/allPost?page=${this.nombrePost}`).then(response => {\n        this.Posts = response.data.data;\n        this.count = response.data.count;\n        this.nbPage = Math.ceil(this.count / 6);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    next() {\n      if (this.nombrePost < this.count - 6) {\n        this.nombrePost += 6;\n        this.page += 1;\n        this.fetchPosts();\n        this.$nextTick(() => {\n          document.querySelector('.sectionHome').scrollIntoView({\n            behavior: 'smooth'\n          });\n        });\n      }\n    },\n    previews() {\n      if (this.nombrePost >= 6) {\n        this.nombrePost -= 6;\n        this.page -= 1;\n        this.fetchPosts();\n        this.$nextTick(() => {\n          document.querySelector('.sectionHome').scrollIntoView({\n            behavior: 'smooth'\n          });\n        });\n      }\n    },\n    getPage(page) {\n      this.nombrePost = page * 6 - 6;\n      this.page = page;\n      this.fetchPosts();\n      this.$nextTick(() => {\n        document.querySelector('.sectionHome').scrollIntoView({\n          behavior: 'smooth'\n        });\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","Posts","nombrePost","page","count","nbPage","mounted","fetchPosts","methods","get","then","response","Math","ceil","catch","error","console","log","next","$nextTick","document","querySelector","scrollIntoView","behavior","previews","getPage"],"sources":["C:\\Users\\Youcode\\Desktop\\FLLLROUGE\\vue\\src\\components\\User\\componentsHome\\AppDisplayQuestion.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        \r\n    