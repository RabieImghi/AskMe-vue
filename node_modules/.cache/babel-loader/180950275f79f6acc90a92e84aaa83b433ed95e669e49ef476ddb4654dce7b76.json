{"ast":null,"code":"import axios from 'axios';\nimport { useStore } from '../../store';\nimport Loader from '../User/AppLoader';\nexport default {\n  name: \"AppManagePost\",\n  data() {\n    return {\n      posts: [],\n      showModal: true,\n      showConfirmModal: false,\n      skip: 0,\n      count: 0,\n      page: 1,\n      nbPage: 1,\n      isLoading: true,\n      postid: null,\n      postTitle: null,\n      postcontent: null\n    };\n  },\n  mounted() {\n    this.getPost();\n  },\n  components: {\n    Loader\n  },\n  methods: {\n    getPost() {\n      const store = useStore();\n      axios.get(`${store.URL}getPostManage/${this.skip}`, {\n        headers: {\n          'Authorization': `Bearer ${store.token}`\n        }\n      }).then(response => {\n        this.posts = response.data.posts;\n        this.count = response.data.count;\n        this.nbPage = Math.ceil(this.count / 6);\n        this.isLoading = false;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    afficheDetail(id, title, content) {\n      this.postid = id;\n      this.postTitle = title;\n      this.postcontent = content;\n      this.showConfirmModal = !this.showConfirmModal;\n    },\n    confirmDelete() {\n      const store = useStore();\n      var formData = new FormData();\n      formData.append('id', this.tageId);\n      axios.post(`${store.URL}deleteTage`, formData, {\n        headers: {\n          'Authorization': `Bearer ${store.token}`\n        }\n      }).then(() => {\n        this.getTages();\n        this.tageName = null;\n        this.showConfirmModal = !this.showConfirmModal;\n      });\n    },\n    nextPage() {\n      if (this.skip < this.count - 6) {\n        this.skip += 6;\n        this.page += 1;\n        this.getTages();\n      }\n    },\n    previewsPage() {\n      if (this.skip >= 6) {\n        this.skip -= 6;\n        this.page -= 1;\n        this.getTages();\n      }\n    },\n    getPage(page) {\n      this.skip = page * 6 - 6;\n      this.page = page;\n      this.getTages();\n    }\n  }\n};","map":{"version":3,"names":["axios","useStore","Loader","name","data","posts","showModal","showConfirmModal","skip","count","page","nbPage","isLoading","postid","postTitle","postcontent","mounted","getPost","components","methods","store","get","URL","headers","token","then","response","Math","ceil","catch","error","console","log","afficheDetail","id","title","content","confirmDelete","formData","FormData","append","tageId","post","getTages","tageName","nextPage","previewsPage","getPage"],"sources":["C:\\Users\\Youcode\\Desktop\\FLLLROUGE\\vue\\src\\components\\Admin\\AppManagePost.vue"],"sourcesContent":["<template>\r\n    <div class=\"test\">\r\n        <div class=\"headerDashboard container-mf pb-1\">\r\n            <span class=\"fw-bold h5 text-dark\">Admin / <span class=\"text-secondary\">Post</span> </span>\r\n        </div>\r\n        <hr>\r\n        <div class=\"container-mf\">\r\n            <div class=\"text-end\">\r\n                <button class=\"btn btn-primary\" @click=\"showModal = !showModal\">Add Tages</button>\r\n            </div>\r\n            <table class=\"w-100\">\r\n                <thead >\r\n                    <tr class=\"itemsPermission\">\r\n                        <th class=\"px-3\">User Name</th>\r\n                        <th class=\"px-3\">Post Category</th>\r\n                        <th class=\"px-3\">Status</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    <tr v-for=\"post in posts\" :key=\"post.id\"  class=\"itemsPermission\"\r\n                    @click=\"afficheDetail(post.id,post.title,post.content)\">\r\n                        <td class=\"px-3\">\r\n                            <p class=\"fw-bold mb-1 text-start\">{{ post.username }}</p>\r\n                        </td>\r\n                        <td class=\"px-3\">\r\n                            <p class=\"fw-bold mb-1 text-start\">{{ post.category }}</p>\r\n   