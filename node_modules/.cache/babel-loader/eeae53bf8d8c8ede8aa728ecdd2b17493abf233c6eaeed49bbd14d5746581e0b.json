lete() {\n      const store = useStore();\n      var formData = new FormData();\n      formData.append('id', this.tageId);\n      axios.post('http://127.0.0.1:8000/api/deleteTage', formData, {\n        headers: {\n          'Authorization': `Bearer ${store.token}`\n        }\n      }).then(() => {\n        this.getTages();\n        this.tageName = null;\n        this.showConfirmModal = !this.showConfirmModal;\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","useStore","name","data","tages","showModal","showModal2","showConfirmModal","tageId","tageName","tageDesc","mounted","getTages","methods","store","get","headers","token","then","response","catch","error","console","log","submitForm","formData","FormData","append","post","submitFormUpdate","deleteTage","id","updateTage","confirmDelete"],"sources":["C:\\Users\\Youcode\\Desktop\\FLLLROUGE\\vue\\src\\components\\Admin\\AppAdminTages.vue"],"sourcesContent":["<template>\r\n    <div class=\"test\">\r\n        <div class=\"headerDashboard container-mf pb-1\">\r\n            <span class=\"fw-bold h5 text-dark\">Admin / <span class=\"text-secondary\">Tages</span> </span>\r\n        </div>\r\n        <hr>\r\n        <div class=\"container-mf\">\r\n            <div class=\"text-end\">\r\n                <button class=\"btn btn-primary\" @click=\"showModal = !showModal\">Add Tages</button>\r\n            </div>\r\n            <table class=\"w-100\">\r\n                <thead >\r\n                    <tr class=\"itemsPermission\">\r\n                        <th class=\"px-3\">Tage Name</th>\r\n                        <th class=\"px-3\">Tage Description</th>\r\n                        <th class=\"px-3\">Action</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    <tr v-for=\"tage in tages\" :key=\"tage.id\"  class=\"itemsPermission\">\r\n                        <td class=\"px-3\">\r\n                            <p class=\"fw-bold mb-1 text-start\">{{ tage