{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      errors: {},\n      formData: {\n        firstname: '',\n        lastname: '',\n        name: '',\n        email: '',\n        password: '',\n        points: 5,\n        role_id: 0\n      }\n    };\n  },\n  methods: {\n    togleInputPassword() {\n      let input = document.getElementById('password');\n      let svg = document.querySelector('svg');\n      if (input.type === 'password') {\n        input.type = 'text';\n        svg.style.fill = 'blue';\n      } else {\n        input.type = 'password';\n        svg.style.fill = 'currentColor';\n      }\n    },\n    createUser() {\n      let form = document.getElementById('form');\n      let data = new FormData(form);\n      let object = {};\n      data.forEach((value, key) => {\n        object[key] = value;\n      });\n      axios.post('http://127.0.0.1:8000/api/RegisterUser', object).then(response => {\n        alert(response.data.data);\n      }).catch(error => {\n        if (error.response.status === 422) {\n          this.errors = error.response.data.errors;\n        }\n        alert(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","data","errors","formData","firstname","lastname","name","email","password","points","role_id","methods","togleInputPassword","input","document","getElementById","svg","querySelector","type","style","fill","createUser","form","FormData","object","forEach","value","key","post","then","response","alert","catch","error","status"],"sources":["C:\\Users\\Youcode\\Desktop\\FLLLROUGE\\fille-rouge-vue\\src\\components\\Auth\\AppAuthRegister.vue"],"sourcesContent":["<template>\r\n    <div class=\"d\">\r\n        <main>\r\n            <div class=\"logo text-center\">\r\n                <img src=\"../../assets/img/logo3.png\" alt=\"logo\" width=\"300px\">\r\n            </div>\r\n            \r\n            <div class=\"cardForm shadow p-5 m-3 \">\r\n                <h3 class=\"blueColor text-center\">Create an Account</h3>\r\n                <p class=\"text-center text-secondary\">Enter your personal details <br> to create account</p>\r\n                <form id=\"form\" class=\"d-flex flex-column justify-content-between gap-4\">\r\n                    <label class=\"text-secondary\"> \r\n                        <input type=\"text\" v-model=\"formData.firstname\" name=\"firstname\"  placeholder=\"First Name ...\" class=\"form-control mt-1\">\r\n                        <span class=\"txet-danger\">{{ errors.firstname }}</span>\r\n                    </label>\r\n                    <label class=\"text-secondary\">\r\n                        <input type=\"text\" v-model=\"form\" name=\"lastname\"  placeholder=\"Last Name ...\" class=\"form-control mt-1\">\r\n                        <span class=\"txet-danger\">{{ errors.lastname }}</span>\r\n                    </label>\r\n                    <input type=\"hidden\" name=\"points\" value=\"5\" id=\"\">\r\n                    <input type=\"hidden\" name=\"role_id\" value=\"0\" id=\"\">\r\n                    <label class=\"text-secondary\">\r\n                        <input type=\"text\" name=\"name\"  placeholder=\"UsrerName ...\" class=\"form-control mt-1\">\r\n                        <span class=\"txet-danger\">{{ errors.name }}</span>\r\n                    </label>\r\n                    <label class=\"text-secondary\">\r\n                        <input type=\"email\" name=\"email\"  placeholder=\"Your Email ...\" class=\"form-control mt-1\">\r\n                        <span class=\"txet-danger\">{{ errors.email }}</span>\r\n                    </label>\r\n                    <label class=\"text-secondary\">\r\n                        <div class=\"input-group mb-3\">\r\n                            <input type=\"password\" name=\"password\" class=\"form-control\" id=\"password\" placeholder=\"Password\">\r\n                            <div class=\"input-group-append cursor-point\">\r\n         