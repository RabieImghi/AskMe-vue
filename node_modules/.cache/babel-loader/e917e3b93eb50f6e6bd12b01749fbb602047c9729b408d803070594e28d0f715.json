etElementById('fileinputProfil').click();\n    },\n    confirmDelete(postId) {\n      Swal.fire({\n        title: 'Are you sure?',\n        text: \"You won't be able to revert this!\",\n        icon: 'warning',\n        showCancelButton: true,\n        confirmButtonColor: '#3085d6',\n        cancelButtonColor: '#d33',\n        confirmButtonText: 'Yes, delete it!'\n      }).then(result => {\n        if (result.isConfirmed) {\n          this.deletePost(postId);\n        }\n      });\n    },\n    deletePost(postId) {\n      axios.delete(`http://127.0.0.1:8000/api/DeletePost/${postId}`).then(response => {\n        Swal.fire('Deleted!', 'Your file has been deleted.', 'success');\n        this.fetchPosts();\n        console.log(response);\n      }).catch(error => {\n        Swal.fire('Error!', 'Your file has not been deleted.', 'error');\n        console.log(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","Swal","useStore","name","data","Posts","nombrePost","page","count","nbPage","mounted","fetchPosts","methods","navigateToUpdate","post","$router","push","params","id","store","get","user_id","then","response","console","log","Math","ceil","catch","error","next","$nextTick","document","querySelector","scrollIntoView","behavior","previews","getPage","uploadsImageCover","getElementById","click","uploadsImageProfile","confirmDelete","postId","fire","title","text","icon","showCancelButton","confirmButtonColor","cancelButtonColor","confirmButtonText","result","isConfirmed","deletePost","delete"],"sources":["C:\\Users\\Youcode\\Desktop\\FLLLROUGE\\vue\\src\\components\\User\\componrntsProfile\\AppMyQuestion.vue"],"sourcesContent":["<template>\r\n    <div class=\"sectionBadge pe-4\">\r\n        <div class=\"bg-white shadow\">\r\n            <section class=\"cover\" >\r\n                <div class=\"coverInfo row p-3\">\r\n                    <div class=\"profile col-2 d-flex flex-column justify-content-end\">\r\n                        <input type=\"file\