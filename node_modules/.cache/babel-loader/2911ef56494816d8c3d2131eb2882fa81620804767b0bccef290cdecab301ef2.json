ue,\r\n                searchQuery: ''\r\n            };\r\n        },\r\n        computed: {\r\n            filteredUsers() {\r\n                return this.tages.filter(tage => {\r\n                    return tage.name.toLowerCase().includes(this.searchQuery.toLowerCase());\r\n                });\r\n            }\r\n        },\r\n        mounted(){\r\n            this.fetchTages();\r\n        },\r\n        methods:{\r\n            fetchTages(){\r\n                const store = useStore();\r\n                let formData = new FormData();\r\n                formData.append('type','user');\r\n                axios.post(`${store.URL}getTages/${this.skip}`, formData).then(response => {\r\n                    this.isLoading = false;\r\n                    this.tages = response.data.tages;\r\n                    this.count = response.data.count;\r\n                    this.nbPage = Math.ceil(this.count / 9);\r\n                });\r\n            },\r\n            nextPage(){\r\n                if(this.skip < this.count - 9){\r\n                    this.skip += 9;\r\n                    this.page += 1;\r\n                    this.fetchTages(); \r\n                    this.$nextTick(() => {\r\n                        document.querySelector('.sectionUser').scrollIntoView({ behavior: 'smooth' });\r\n                    });\r\n                }\r\n            },\r\n            previewsPage(){\r\n                if(this.skip >= 9){\r\n                    this.skip -= 9;\r\n                    this.page -= 1;\r\n                    this.fetchTages();\r\n                    this.$nextTick(() => {\r\n                        document.querySelector('.sectionUser').scrollIntoView({ behavior: 'smooth' });\r\n                    });\r\n                }\r\n            },\r\n            getPage(page){\r\n                this.skip = page * 9 - 9;\r\n                this.page = page;\r\n                this.fetchTages();\r\n                this.$nextTick(() => {\r\n                    document.querySelector('.sectionUser').scrollIntoView({ behavior: 'smooth' });\r\n                });\r\n            },\r\n        }\r\n        \r\n    }\r\n</script>\r\n<style>\r\n    .sectionBadge{\r\n        min-height: calc( 100vh - 80px);\r\n    }\r\n    .tagelists{\r\n        min-width: 360px;\r\n        max-width: 360px;\r\n    }\r\n</style>"],"mappings":"AAgDI,OAAOA,KAAI,MAAO,OAAO;AACzB,SAAQC,QAAQ,QAAO,SAAS;AAChC,eAAc;EACVC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAE;IACF,MAAMC,KAAI,GAAIH,QAAQ,CAAC,CAAC;IACxB,OAAM;MACFI,KAAI,EAAI,EAAE;MACVC,IAAI,EAAEF,KAAK,CAACE,IAAI;MAChBC,KAAK,EAAC,CAAC;MACPC,IAAI,EAAC,CAAC;MACNC,MAAM,EAAC,CAAC;MACRC,SAAS,EAAE,IAAI;MACfC,WAAW,EAAE;IACjB,CAAC;EACL,CAAC;EACDC,QAAQ,EAAE;IACNC,aAAaA,CAAA,EAAG;MACZ,OAAO,IAAI,CAACR,KAAK,CAACS,MAAM,CAACC,IAAG,IAAK;QAC7B,OAAOA,IAAI,CAACb,IAAI,CAACc,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACN,WAAW,CAACK,WAAW,CAAC,CAAC,CAAC;MAC3E,CAAC,CAAC;IACN;EACJ,CAAC;EACDE,OAAOA,CAAA,EAAE;IACL,IAAI,CAACC,UAAU,CAAC,CAAC;EACrB,CAAC;EACDC,OAAO,EAAC;IACJD,UAAUA,CAAA,EAAE;MACR,MAAMf,KAAI,GAAIH,QAAQ,CAAC,CAAC;MACxB,IAAIoB,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC7BD,QAAQ,CAACE,MAAM,CAAC,MAAM,EAAC,MAAM,CAAC;MAC9BvB,KAAK,CAACwB,IAAI,CAAE,GAAEpB,KAAK,CAACqB,GAAI,YAAW,IAAI,CAACnB,IAAK,EAAC,EAAEe,QAAQ,CAAC,CAACK,IAAI,CAACC,QAAO,IAAK;QACvE,IAAI,CAACjB,SAAQ,GAAI,KAAK;QACtB,IAAI,CAACL,KAAI,GAAIsB,QAAQ,CAACxB,IAAI,CAACE,KAAK;QAChC,IAAI,CAACE,KAAI,GAAIoB,QAAQ,CAACxB,IAAI,CAACI,KAAK;QAChC,IAAI,CAACE,MAAK,GAAImB,IAAI,CAACC,IAAI,CAAC,IAAI,CAACtB,KAAI,GAAI,CAAC,CAAC;MAC3C,CAAC,CAAC;IACN,CAAC;IACDuB,QAAQA,CAAA,EAAE;MACN,IAAG,IAAI,CAACxB,IAAG,GAAI,IAAI,CAACC,KAAI,GAAI,CAAC,EAAC;QAC1B,IAAI,CAACD,IAAG,IAAK,CAAC;QACd,IAAI,CAACE,IAAG,IAAK,CAAC;QACd,IAAI,CAACW,UAAU,CAAC,CAAC;QACjB,IAAI,CAACY,SAAS,CAAC,MAAM;UACjBC,QAAQ,CAACC,aAAa,CAAC,cAAc,CAAC,CAACC,cAAc,CAAC;YAAEC,QAAQ,EAAE;UAAS,CAAC,CAAC;QACjF,CAAC,CAAC;MACN;IACJ,CAAC;IACDC,YAAYA,CAAA,EAAE;MACV,IAAG,IAAI,CAAC9B,IAAG,IAAK,CAAC,EAAC;QACd,IAAI,CAACA,IAAG,IAAK,CAAC;QACd,IAAI,CAACE,IAAG,IAAK,CAAC;QACd,IAAI,