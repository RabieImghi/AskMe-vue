{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport Swal from 'sweetalert2';\nimport { useStore } from '@/store';\nexport default {\n  name: 'AppBadge',\n  data() {\n    return {\n      Posts: [],\n      nombrePost: 0,\n      page: 1,\n      count: 0,\n      nbPage: 1,\n      userId: null\n    };\n  },\n  mounted() {\n    const store = new useStore();\n    this.userId = store.user_id;\n    this.fetchPosts();\n  },\n  methods: {\n    navigateToAnswer(postId) {\n      this.$router.push({\n        name: 'userAnswers',\n        params: {\n          id: postId\n        }\n      });\n    },\n    async ChangeReating(type, id) {\n      var store = new useStore();\n      var idUser = store.user_id;\n      let response = await axios.get(`http://127.0.0.1:8000/api/ChangeReating/${id}/${idUser}/${type}`, {\n        headers: {\n          'Authorization': `Bearer ${store.token}`\n        }\n      });\n      if (response == 200) {\n        this.fetchPosts();\n      }\n    },\n    navigateToUpdate(post) {\n      this.$router.push({\n        name: 'UpdateMyQuestion',\n        params: {\n          id: post.id,\n          titlePoste: post.title,\n          content: post.content,\n          category_id: post.category_id,\n          tages_id: post.tages_id\n        }\n      });\n    },\n    async fetchPosts() {\n      var store = new useStore();\n      let response = await axios.get(`http://127.0.0.1:8000/api/MyPost/${store.user_id}?page=${this.nombrePost}`, {\n        headers: {\n          'Authorization': `Bearer ${store.token}`\n        }\n      });\n      if (response.status == 200) {\n        this.Posts = response.data.data;\n        this.count = response.data.count;\n        this.nbPage = Math.ceil(this.count / 6);\n      }\n    },\n    next() {\n      if (this.nombrePost < this.count - 6) {\n        this.nombrePost += 6;\n        this.page += 1;\n        this.fetchPosts();\n        this.$nextTick(() => {\n          document.querySelector('.sectionHome').scrollIntoView({\n            behavior: 'smooth'\n          });\n        });\n      }\n    },\n    previews() {\n      if (this.nombrePost >= 6) {\n        this.nombrePost -= 6;\n        this.page -= 1;\n        this.fetchPosts();\n        this.$nextTick(() => {\n          document.querySelector('.sectionHome').scrollIntoView({\n            behavior: 'smooth'\n          });\n        });\n      }\n    },\n    getPage(page) {\n      this.nombrePost = page * 6 - 6;\n      this.page = page;\n      this.fetchPosts();\n      this.$nextTick(() => {\n        document.querySelector('.sectionHome').scrollIntoView({\n          behavior: 'smooth'\n        });\n      });\n    },\n    uploadsImageCover() {\n      document.getElementById('fileinput').click();\n    },\n    uploadsImageProfile() {\n      document.getElementById('fileinputProfil').click();\n    },\n    confirmDelete(postId) {\n      Swal.fire({\n        title: 'Are you sure?',\n        text: \"You won't be able to revert this!\",\n        icon: 'warning',\n        showCancelButton: true,\n        confirmButtonColor: '#3085d6',\n        cancelButtonColor: '#d33',\n        confirmButtonText: 'Yes, delete it!'\n      }).then(result => {\n        if (result.isConfirmed) {\n          this.deletePost(postId);\n        }\n      });\n    },\n    async deletePost(postId) {\n      let store = new useStore();\n      let response = await axios.delete(`http://127.0.0.1:8000/api/DeletePost/${postId}`, {\n        headers: {\n          'Authorization': `Bearer ${store.token}`\n        }\n      });\n      if (response.status == 200) {\n        Swal.fire('Deleted!', 'Your post has been deleted.', 'success');\n        this.fetchPosts();\n        console.log(response);\n      } else {\n        Swal.fire('Error!', 'Your post has not been deleted.', 'error');\n      }\n    },\n    isInArray(idUser, table) {\n      let idTable = table.map(item => String(item.id));\n      let idUserExists = idTable.includes(String(idUser));\n      if (idUserExists) {\n        var voteType = table.filter(item => item.id == idUser);\n        if (voteType[0].type == '+') {\n          return 'Active+';\n        } else {\n          return 'Active-';\n        }\n      } else return '';\n    }\n  }\n};","map":{"version":3,"names":["axios","Swal","useStore","name","data","Posts","nombrePost","page","count","nbPage","userId","mounted","store","user_id","fetchPosts","methods","navigateToAnswer","postId","$router","push","params","id","ChangeReating","type","idUser","response","get","headers","token","navigateToUpdate","post","titlePoste","title","content","category_id","tages_id","status","Math","ceil","next","$nextTick","document","querySelector","scrollIntoView","behavior","previews","getPage","uploadsImageCover","getElementById","click","uploadsImageProfile","confirmDelete","fire","text","icon","showCancelButton","confirmButtonColor","cancelButtonColor","confirmButtonText","then","result","isConfirmed","deletePost","delete","console","log","isInArray","table","idTable","map","item","String","idUserExists","includes","voteType","filter"],"sources":["C:\\Users\\Youcode\\Desktop\\FLLLROUGE\\vue\\src\\components\\User\\componrntsProfile\\AppMyQuestion.vue"],"sourcesContent":["<template>\r\n    <div class=\"sectionBadge pe-4\">\r\n        <div class=\"bg-white shadow\">\r\n            <div class=\"border-bottom container-mf pt-4 pb-4 d-flex justify-content-between align-items-center\">\r\n                <span class=\"fw-bold text-secondary\">Home /<span class=\"text-secondary-500\"> My Question</span></span>  \r\n                <!-- <button class=\"btn btn-light shadow fw-bold text-secondary\">Show All Your Question</button> -->\r\n            </div> \r\n            \r\n            <section class=\"container-mf mt-4 pt-2\">\r\n                <div class=\"border-bottom pb-4 pt-4\" v-for=\"post in Posts\" :key=\"post.id\">\r\n                    <div class=\"container-mf mobileQuestion row\">\r\n                        <div class=\"imageInfoUser col-1 gap-3 d-flex flex-column justify-content-start align-items-center\">\r\n                            <router-link :to=\"{ name: 'UserProfile', params: { idUser: post.user_id } }\">\r\n                                <img :src=\"post.imageUser\" style=\"border-radius: 50%;\" width=\"80px\" alt=\"User\">\r\n                            </router-link>\r\n                            <div class=\"raitting d-flex flex-column  justify-content-center align-items-center gap-2\">\r\n                                <span class=\"cursor-point\" @click=\"ChangeReating('+',post.id)\">\r\n                                    <svg :class=\"{ 'activeVote': isInArray(this.userId, post.listIdUserVoted) === 'Active+' }\" id=plusVote xmlns=\"http://www.w3.org/2000/svg\" width=\"20\" height=\"20\" fill=\"currentColor\" class=\"bi bi-arrow-up-circle\" viewBox=\"0 0 16 16\">\r\n                                        <path fill-rule=\"evenodd\" d=\"M1 8a7 7 0 1 0 14 0A7 7 0 0 0 1 8m15 0A8 8 0 1 1 0 8a8 8 0 0 1 16 0m-7.5 3.5a.5.5 0 0 1-1 0V5.707L5.354 7.854a.5.5 0 1 1-.708-.708l3-3a.5.5 0 0 1 .708 0l3 3a.5.5 0 0 1-.708.708L8.5 5.707z\"/>\r\n                                    </svg>\r\n                                </span>\r\n                                <span class=\"text-secondary fw-bold\">{{post.reating}}</span>\r\n                                <span class=\"cursor-point\" @click=\"ChangeReating('-',post.id)\">\r\n                                    <svg :class=\"{ 'activeVote': isInArray(this.userId, post.listIdUserVoted) === 'Active-' }\" id=\"moinVote\" xmlns=\"http://www.w3.org/2000/svg\" width=\"20\" height=\"20\" fill=\"currentColor\" class=\"bi bi-arrow-down-circle\" viewBox=\"0 0 16 16\">\r\n                                        <path fill-rule=\"evenodd\" d=\"M1 8a7 7 0 1 0 14 0A7 7 0 0 0 1 8m15 0A8 8 0 1 1 0 8a8 8 0 0 1 16 0M8.5 4.5a.5.5 0 0 0-1 0v5.793L5.354 8.146a.5.5 0 1 0-.708.708l3 3a.5.5 0 0 0 .708 0l3-3a.5.5 0 0 0-.70