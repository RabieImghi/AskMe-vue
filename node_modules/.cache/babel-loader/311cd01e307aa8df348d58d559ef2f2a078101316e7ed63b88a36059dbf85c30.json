ooter\">\r\n                        <button type=\"button\" class=\"btn btn-secondary\" @click=\"showConfirmModal = false\">No</button>\r\n                        <button type=\"button\" class=\"btn btn-danger\" @click=\"confirmDelete\">Confirme</button>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n<style scoped>\r\n    table {\r\n        border-collapse: separate;\r\n        border-spacing: 0 20px;\r\n    }\r\n\r\n    table tr {\r\n        box-shadow: 2px 3px 10px rgba(211, 211, 211, 0.635);\r\n        border-radius: 3px;\r\n        padding: 20px;\r\n    }\r\n    table tr:hover {\r\n        cursor: pointer;\r\n        transform: translateY(-2px);\r\n        background-color: #f5f5f580;\r\n    }\r\n    table td, table th {\r\n        padding: 10px 0;\r\n    }\r\n    .model {\r\n        position: fixed;\r\n        right: 0;\r\n        top: 10vh;\r\n        width: 400px;\r\n        height: 90vh;\r\n        background-color: white;\r\n        box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n        transform: translateX(100%);\r\n        transition: transform 0.3s ease-out;\r\n        overflow: auto;\r\n    }\r\n    .shad {\r\n        background-color: rgba(75, 75, 75, 0.253);\r\n    }\r\n    .confirm{\r\n        background: white;\r\n        margin-top: 100px;\r\n    }\r\n    .model.show {\r\n        display: block !important;\r\n        transform: translateX(0);\r\n    }\r\n    .overlay.show {\r\n        background: rgba(0, 0, 0, 0.5);\r\n        position: fixed;\r\n        top: 10vh;\r\n        left: 0;\r\n        width: 100%;\r\n        height: 90vh;\r\n        transform: translateX(0);\r\n        transition: transform 0.3s ease-out;\r\n        opacity: 0.2;\r\n    }\r\n    .prmissions{\r\n        background-color: #f0f0f0;\r\n        padding: 5px 10px;\r\n        border-radius: 5px;\r\n    }\r\n    .prmissions:hover{\r\n        background-color: #e0e0e0;\r\n        transform: translateY(